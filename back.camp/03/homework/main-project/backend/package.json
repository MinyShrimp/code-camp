{
    "name": "main-project",
    "version": "0.0.1",
    "description": "",
    "author": "",
    "private": true,
    "license": "UNLICENSED",
    "scripts": {
        "prebuild": "rimraf dist",
        "build": "nest build",
        "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
        "start": "nest start",
        "start:dev": "nest start --watch",
        "start:debug": "nest start --debug --watch",
        "start:prod": "node dist/main",
        "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "test:e2e": "jest --config ./test/jest-e2e.json",
        "start:admin": "nodemon --watch src --exec 'npx' 'ts-node' ./src/admin.ts"
    },
    "dependencies": {
        "@google-cloud/storage": "6.1.0",
        "@nestjs/apollo": "^10.0.14",
        "@nestjs/common": "^8.0.0",
        "@nestjs/config": "^2.1.0",
        "@nestjs/core": "^8.0.0",
        "@nestjs/graphql": "^10.0.15",
        "@nestjs/jwt": "^8.0.1",
        "@nestjs/passport": "^8.2.1",
        "@nestjs/platform-express": "^8.0.0",
        "@nestjs/typeorm": "^8.1.2",
        "apollo-server-express": "^3.8.2",
        "axios": "^0.27.2",
        "bcryptjs": "^2.4.3",
        "class-transformer": "^0.5.1",
        "class-validator": "^0.13.2",
        "express-session": "^1.17.3",
        "graphql": "^16.5.0",
        "graphql-upload": "13.0.0",
        "iamport-server-api": "^1.0.2",
        "mysql2": "^2.3.3",
        "passport": "0.5.0",
        "passport-google-oauth20": "^2.0.0",
        "passport-jwt": "^4.0.0",
        "passport-kakao": "^1.0.1",
        "passport-naver": "^1.0.6",
        "reflect-metadata": "^0.1.13",
        "rimraf": "^3.0.2",
        "rxjs": "^7.2.0",
        "typeorm": "0.2"
    },
    "devDependencies": {
        "@nestjs/cli": "^8.0.0",
        "@nestjs/schematics": "^8.0.0",
        "@nestjs/testing": "^8.0.0",
        "@rollup/plugin-commonjs": "^22.0.0",
        "@rollup/plugin-node-resolve": "^13.3.0",
        "@types/axios": "^0.14.0",
        "@types/bcryptjs": "^2.4.2",
        "@types/express": "^4.17.13",
        "@types/graphql-upload": "^8.0.11",
        "@types/jest": "27.4.1",
        "@types/node": "^16.0.0",
        "@types/passport-google-oauth20": "^2.0.11",
        "@types/passport-jwt": "^3.0.6",
        "@types/passport-kakao": "^0.2.1",
        "@types/passport-naver": "^0.2.1",
        "@types/supertest": "^2.0.11",
        "@typescript-eslint/eslint-plugin": "^5.0.0",
        "@typescript-eslint/parser": "^5.0.0",
        "eslint": "^8.0.1",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-prettier": "^4.0.0",
        "jest": "^27.2.5",
        "nodemon": "^2.0.16",
        "prettier": "^2.3.2",
        "source-map-support": "^0.5.20",
        "supertest": "^6.1.3",
        "ts-jest": "^27.0.3",
        "ts-node": "^10.0.0",
        "tsconfig-paths": "^3.10.1",
        "typescript": "^4.3.5"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "rootDir": "src",
        "testRegex": ".*\\.spec\\.ts$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "collectCoverageFrom": [
            "**/*.(t|j)s"
        ],
        "coverageDirectory": "../coverage",
        "testEnvironment": "node"
    }
}
